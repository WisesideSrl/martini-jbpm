<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                   xmlns:drools="http://www.jboss.org/drools"
                   xmlns:tns="http://www.jboss.org/drools"
                   id="_ProcFiglio"
                   targetNamespace="http://www.jboss.org/drools">
  
  <bpmn2:itemDefinition id="_ordineIdItem" structureRef="String"/>
  <bpmn2:itemDefinition id="_lottoNumeroItem" structureRef="String"/>
  <bpmn2:itemDefinition id="_risultatoItem" structureRef="String"/>
  
  <bpmn2:process id="com.martinispec.procfiglio" 
                 drools:packageName="com.martinispec.processes" 
                 drools:version="1.0" 
                 name="ProcFiglio" 
                 isExecutable="true">
    
    <bpmn2:property id="ordineId" itemSubjectRef="_ordineIdItem" name="ordineId"/>
    <bpmn2:property id="lottoNumero" itemSubjectRef="_lottoNumeroItem" name="lottoNumero"/>
    <bpmn2:property id="risultato" itemSubjectRef="_risultatoItem" name="risultato"/>
    
    <bpmn2:startEvent id="_startEvent" name="Start">
      <bpmn2:outgoing>_flow1</bpmn2:outgoing>
    </bpmn2:startEvent>
    
    <bpmn2:scriptTask id="_logDati" name="Log Dati Ricevuti" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>_flow1</bpmn2:incoming>
      <bpmn2:outgoing>_flow2</bpmn2:outgoing>
      <bpmn2:script><![CDATA[
System.out.println("[ProcFiglio] ======================================");
System.out.println("[ProcFiglio] PROCESSO AVVIATO VIA JMS!");
System.out.println("[ProcFiglio] Dati ricevuti dal padre:");
System.out.println("[ProcFiglio] ordineId: " + ordineId);
System.out.println("[ProcFiglio] lottoNumero: " + lottoNumero);
System.out.println("[ProcFiglio] Process Instance ID: " + kcontext.getProcessInstance().getId());
System.out.println("[ProcFiglio] ======================================");
]]></bpmn2:script>
    </bpmn2:scriptTask>
    
    <bpmn2:scriptTask id="_elaboraOrdine" name="Elabora Ordine" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>_flow2</bpmn2:incoming>
      <bpmn2:outgoing>_flow3</bpmn2:outgoing>
      <bpmn2:script><![CDATA[
System.out.println("[ProcFiglio] ======================================");
System.out.println("[ProcFiglio] Elaborazione ordine " + ordineId);
System.out.println("[ProcFiglio] Lotto " + lottoNumero + " in processamento");

try {
    Thread.sleep(3000);
} catch (InterruptedException e) {
    e.printStackTrace();
}

String risultatoElaborazione = "OK - Ordine " + ordineId + " elaborato con successo";
kcontext.setVariable("risultato", risultatoElaborazione);

System.out.println("[ProcFiglio] Elaborazione completata!");
System.out.println("[ProcFiglio] Risultato: " + risultatoElaborazione);
System.out.println("[ProcFiglio] ======================================");
]]></bpmn2:script>
    </bpmn2:scriptTask>
    
    <bpmn2:task id="_notificaPadre" name="Notifica Padre (JMS)" tns:taskName="JMS Send Message">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Notifica Padre (JMS)]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_flow3</bpmn2:incoming>
      <bpmn2:outgoing>_flow4</bpmn2:outgoing>
      <bpmn2:ioSpecification>
        <bpmn2:dataInput id="_notificaPadre_messageNameInput" drools:dtype="String" itemSubjectRef="__notificaPadre_messageNameInputItem" name="messageName"/>
        <bpmn2:dataInput id="_notificaPadre_correlationKeysInput" drools:dtype="Object" itemSubjectRef="__notificaPadre_correlationKeysInputItem" name="correlationKeys"/>
        <bpmn2:dataInput id="_notificaPadre_payloadInput" drools:dtype="Object" itemSubjectRef="__notificaPadre_payloadInputItem" name="payload"/>
        <bpmn2:inputSet>
          <bpmn2:dataInputRefs>_notificaPadre_messageNameInput</bpmn2:dataInputRefs>
          <bpmn2:dataInputRefs>_notificaPadre_correlationKeysInput</bpmn2:dataInputRefs>
          <bpmn2:dataInputRefs>_notificaPadre_payloadInput</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation>
        <bpmn2:targetRef>_notificaPadre_messageNameInput</bpmn2:targetRef>
        <bpmn2:assignment>
          <bpmn2:from xsi:type="bpmn2:tFormalExpression"><![CDATA[figlioCompletato]]></bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">_notificaPadre_messageNameInput</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataInputAssociation>
        <bpmn2:targetRef>_notificaPadre_correlationKeysInput</bpmn2:targetRef>
        <bpmn2:assignment>
          <bpmn2:from xsi:type="bpmn2:tFormalExpression"><![CDATA[["ordineId":ordineId]]]></bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">_notificaPadre_correlationKeysInput</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataInputAssociation>
        <bpmn2:targetRef>_notificaPadre_payloadInput</bpmn2:targetRef>
        <bpmn2:assignment>
          <bpmn2:from xsi:type="bpmn2:tFormalExpression"><![CDATA[["risultato":risultato]]]></bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">_notificaPadre_payloadInput</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
    </bpmn2:task>
    
    <bpmn2:scriptTask id="_logCompletamento" name="Log Completamento" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>_flow4</bpmn2:incoming>
      <bpmn2:outgoing>_flow5</bpmn2:outgoing>
      <bpmn2:script><![CDATA[
System.out.println("[ProcFiglio] ======================================");
System.out.println("[ProcFiglio] Notifica inviata al padre via JMS");
System.out.println("[ProcFiglio] messageName: figlioCompletato");
System.out.println("[ProcFiglio] correlationKey: ordineId=" + ordineId);
System.out.println("[ProcFiglio] payload: risultato=" + risultato);
System.out.println("[ProcFiglio] PROCESSO FIGLIO TERMINATO");
System.out.println("[ProcFiglio] ======================================");
]]></bpmn2:script>
    </bpmn2:scriptTask>
    
    <bpmn2:endEvent id="_endEvent" name="End">
      <bpmn2:incoming>_flow5</bpmn2:incoming>
    </bpmn2:endEvent>
    
    <bpmn2:sequenceFlow id="_flow1" sourceRef="_startEvent" targetRef="_logDati"/>
    <bpmn2:sequenceFlow id="_flow2" sourceRef="_logDati" targetRef="_elaboraOrdine"/>
    <bpmn2:sequenceFlow id="_flow3" sourceRef="_elaboraOrdine" targetRef="_notificaPadre"/>
    <bpmn2:sequenceFlow id="_flow4" sourceRef="_notificaPadre" targetRef="_logCompletamento"/>
    <bpmn2:sequenceFlow id="_flow5" sourceRef="_logCompletamento" targetRef="_endEvent"/>
    
  </bpmn2:process>
  
  <bpmn2:itemDefinition id="__notificaPadre_messageNameInputItem" structureRef="String"/>
  <bpmn2:itemDefinition id="__notificaPadre_correlationKeysInputItem" structureRef="Object"/>
  <bpmn2:itemDefinition id="__notificaPadre_payloadInputItem" structureRef="Object"/>
  
  <bpmndi:BPMNDiagram>
    <bpmndi:BPMNPlane bpmnElement="com.martinispec.procfiglio">
      <bpmndi:BPMNShape id="_startEvent_di" bpmnElement="_startEvent">
        <dc:Bounds x="50" y="100" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="_logDati_di" bpmnElement="_logDati">
        <dc:Bounds x="136" y="78" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="_elaboraOrdine_di" bpmnElement="_elaboraOrdine">
        <dc:Bounds x="286" y="78" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="_notificaPadre_di" bpmnElement="_notificaPadre">
        <dc:Bounds x="436" y="78" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="_logCompletamento_di" bpmnElement="_logCompletamento">
        <dc:Bounds x="586" y="78" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="_endEvent_di" bpmnElement="_endEvent">
        <dc:Bounds x="736" y="100" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="_flow1_di" bpmnElement="_flow1">
        <di:waypoint x="68" y="118"/>
        <di:waypoint x="186" y="118"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="_flow2_di" bpmnElement="_flow2">
        <di:waypoint x="236" y="118"/>
        <di:waypoint x="336" y="118"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="_flow3_di" bpmnElement="_flow3">
        <di:waypoint x="386" y="118"/>
        <di:waypoint x="486" y="118"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="_flow4_di" bpmnElement="_flow4">
        <di:waypoint x="536" y="118"/>
        <di:waypoint x="636" y="118"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="_flow5_di" bpmnElement="_flow5">
        <di:waypoint x="686" y="118"/>
        <di:waypoint x="754" y="118"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
  
</bpmn2:definitions>
